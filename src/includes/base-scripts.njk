{# Bootstrap #}
<script
  src="{%- if isdevelopment -%}
    /static/vendor/bootstrap-5.3.2/dist/js/bootstrap.js
  {%- else -%}
    https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js
  {%- endif -%}"
  type="text/javascript"
></script>

{# JQuery (for Owl-carousel only) #}
<script
  src="{%- if isdevelopment -%}
    /static/vendor/jquery-3.5.1/jquery-3.5.1.js
  {%- else -%}
    https://code.jquery.com/jquery-3.5.1.min.js
  {%- endif -%}"
  type="text/javascript"
></script>

{# Owl-carousel #}
<script
  src="{%- if isdevelopment -%}
    /static/vendor/owl-carousel-2.3.4/owl.carousel.js
  {%- else -%}
    https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js
  {%- endif -%}"
  type="text/javascript"
></script>

{% include "main/visualization-assets.njk" %}

