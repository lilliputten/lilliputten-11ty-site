  <script>
    window.isDev = true;
  </script>
{% endif %}

{% if isIndex %}
  <script>
    window.isIndex = true;
  </script>
{% endif %}

{# Compiled scripts #}
<script type="text/javascript" src="/compiled-assets/scripts.js?{{ site.version }}"></script>

